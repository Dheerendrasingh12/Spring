<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="dtdc" class="com.dheerendra.beans.DTDC"></bean>
	<bean id="blueDart" class="com.dheerendra.beans.BlueDart"></bean>

	<bean id="fpk" class="com.dheerendra.beans.Flipkart">
		<!-- <property name="courier" ref="dtdc"></property> -->
		<property name="courier" ref="blueDart"></property>
	</bean>
</beans>